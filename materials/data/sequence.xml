<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Cost Sheet Sequence -->
        <record id="seq_project_cost_sheet" model="ir.sequence">
            <field name="name">Cost Sheet</field>
            <field name="code">project.cost.sheet</field>
            <field name="prefix">CS/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Stock Request Sequence -->
        <record id="seq_stock_request" model="ir.sequence">
            <field name="name">Stock Request</field>
            <field name="code">stock.request</field>
            <field name="prefix">SRO/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>